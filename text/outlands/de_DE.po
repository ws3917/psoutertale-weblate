msgstr "<32>{#p/asriel1}* \"Asriel's Tagebuch, K-515.09\""

msgid "<32>* \"$(name) said it's time for the plan.\""
msgstr "<32>* \"$(name) hat gesagt, es ist Zeit für den Plan.\""

