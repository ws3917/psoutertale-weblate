msgstr "<18>{#f/4}SIN MENCIONAR QUE, ME ESTOY CANSANDO DE CAPTURARTE."

msgid "<18>{#f/5}WOULD YOU LIKE TO PASS THROUGH..."
msgstr "<18>{#f/5}TE GUSTARÍA PASAR..."

msgid "<18>{#f/5}... WITHOUT A BATTLE?"
msgstr "<18>{#f/5}... ¿SIN UNA BATALLA?"

msgid ""
"<18>{#p/papyrus}{#f/31}...\n"
"OKAY..."
msgstr ""
"<18>{#p/papyrus}{#f/31}...\n"
"ESTA BIEN..."

msgid "<18>{#f/3}I GUESS I'LL ACCEPT MY FAILURE."
msgstr "<18>{#f/3}SUPONGO QUE ACEPTARÉ MI FRACASO."

msgid "<18>{#p/papyrus}{#f/4}WELL, IF YOU SAY SO, THEN..."
msgstr "<18>{#p/papyrus}{#f/4}BUENO, SI TÚ LO DICES, ENTONCES..."

msgid "<18>{#f/9}BY ALL MEANS!!!"
msgstr "<18>{#f/9}¡¡¡ADELANTE!!!"

msgid "<18>{#p/papyrus}{#f/1}AGAIN??"
msgstr "<18>{#p/papyrus}{#f/1}¿¿DE NUEVO??"

msgid "<18>{#f/4}... WELL, OKAY..."
msgstr "<18>{#f/4}... BUENO, ESTÁ BIEN..."

