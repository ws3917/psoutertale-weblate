msgid "<25>{#p/undyne}{#f/4}* ..."
msgstr ""

msgid "<25>{#p/undyne}{#f/5}* ..."
msgstr ""

msgid "<25>{#p/undyne}{#f/12}* ... okay, didn't see that one coming."
msgstr ""

